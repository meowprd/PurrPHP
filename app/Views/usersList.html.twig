{% extends "default.html.twig" %}

{% block main %}

<table>
  <tr>
    <th>ID</th>
    <th>Name</th>
    <th>Created At</th>
  </tr>
  {% for user in users %}
    <tr>
      <td>{{ user.id }}</td><td>{{ user.name }}</td><td>{{ user.createdAt|date('Y-m-d H:i:s') }}</td>
    </tr>
  {% endfor %}
</table>

{% endblock main %}